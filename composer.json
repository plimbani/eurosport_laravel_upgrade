{
    "name": "laravel/laravel",
    "description": "The Laravel Framework.",
    "keywords": ["framework", "laravel"],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": ">=5.6.4",
        "laravel/framework": "5.3.*",
        "barryvdh/laravel-debugbar": "^2.3",
        "laravelcollective/html": "^5.3.0",
        "duro85/roles": "^2.1",
        "jtant/laravel-env-sync": "^1.3",
        "brotzka/laravel-dotenv-editor": "2.*",
        "laracasts/utilities": "^2.1",
        "spatie/laravel-collection-macros": "^1.5",
        "spatie/laravel-activitylog": "^1.10",
        "spatie/laravel-backup": "^4.3",
        "spatie/valuestore": "^1.0",
        "spatie/eloquent-sortable": "^3.1",
        "spatie/laravel-medialibrary": "^4.9",
        "spatie/laravel-tags": "^1.2",
        "spatie/laravel-sluggable": "^1.3",
        "spatie/url-signer": "^1.0",
        "spatie/url-signer": "^1.0",
        "vinkla/hashids": "^3.0",
        "anhskohbo/no-captcha": "^2.1",
        "maatwebsite/excel": "^2.1",
        "propaganistas/laravel-phone": "^2.8",
        "rinvex/country": "^2.0",
        "iatstuti/laravel-cascade-soft-deletes": "^1.1",
        "rinvex/repository": "^2.0",
        "dingo/api": "1.0.*@dev",
        "tymon/jwt-auth": "0.5.*",
        "phpro/grumphp": "dev-master"
    },
    "require-dev": {
        "fzaninotto/faker": "~1.6",
        "mockery/mockery": "0.9.*",
        "phpunit/phpunit": "~5.0",
        "symfony/css-selector": "3.2.*",
        "symfony/dom-crawler": "3.2.*",
        "squizlabs/php_codesniffer": "3.0.x-dev",
        "codeception/codeception": "^2.2",
        "sebastian/phpcpd": "^2.0",
        "friendsofphp/php-cs-fixer": "^2.0",
        "roave/security-advisories": "dev-master",
        "sensiolabs/security-checker": "^4.0",
        "phpmd/phpmd": "dev-master",
        "nikic/php-parser": "^2.1",
        "phpspec/phpspec": "^3.2",
        "jakub-onderka/php-parallel-lint": "^0.9.2",
        "garygreen/pretty-routes": "^0.0.5",
        "jeroen-g/laravel-packager": "^1.5",
        "mpociot/laravel-test-factory-helper": "^0.3.1"
    },
    "autoload": {
        "classmap": [
            "database"
        ],
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "classmap": [
            "tests/TestCase.php"
        ]
    },
    "scripts": {
        "post-root-package-install": [
            "php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "php artisan key:generate"
        ],
        "post-install-cmd": [
            "Illuminate\\Foundation\\ComposerScripts::postInstall",
            "php artisan optimize"
        ],
        "post-update-cmd": [
            "Illuminate\\Foundation\\ComposerScripts::postUpdate",
            "php artisan optimize"
        ]
    },
    "config": {
        "minimum-stability": "dev"
    }
}
