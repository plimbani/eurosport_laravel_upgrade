
platform :ios, '13.0'

use_frameworks!

def pods_list

  pod 'SDWebImage', '~> 4.0'
  pod 'GoogleMaps'
  pod 'Firebase/Core'
  pod 'Firebase/Messaging'
  pod "MBCircularProgressBar"
  pod 'Firebase/Crashlytics'
  pod 'Firebase/Analytics'
  
end

target 'ESR' do
  pods_list
end

target 'ESR comm' do
  pods_list
end
